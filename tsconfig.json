{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "ES2015"
    ],
    "downlevelIteration": true,
    "allowJs": true,
    "checkJs": false,
    "outDir": "./public/js",
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "baseUrl": "./",
    "rootDirs": [
      "src"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*"
  ]
}
